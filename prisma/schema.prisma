  generator client {
    provider = "prisma-client-js"
  }

  datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
  }

  model courseList {
    courseId         String     @db.VarChar(255) @id @unique
    name             String     @db.VarChar(255)
    category         Category
    difficulty       Difficulty
    courseOutput     Json
    createdAt        DateTime   @default(now())
    createdBy        String     @db.VarChar(255)
    userProfileImage String     @db.VarChar(255)
  }

  enum Category {
  health
  business
  technology
  programming
  science
  creative
}

enum Difficulty {
  beginner
  intermediate
  advance
}

